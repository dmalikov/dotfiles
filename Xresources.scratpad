URxvt_scratchpad.color10: #7B996C
URxvt_scratchpad.color3:  #7C5618
URxvt_scratchpad.color11: #B38849
URxvt_scratchpad.color4:  #223535
URxvt_scratchpad.color12: #889393
URxvt_scratchpad.color5:  #8E4948
URxvt_scratchpad.color13: #C67C78
URxvt_scratchpad.color6:  #6D5B42
URxvt_scratchpad.color14: #A28D73
URxvt_scratchpad.color7:  #8D8D8D
URxvt_scratchpad.color15: #C6C6C6

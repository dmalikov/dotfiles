! xmonad scratchpad settings
URxvt_scratchpad.background: #111111
URxvt_scratchpad.foreground: #a1a1a1

URxvt_scratchpad.perl-ext-common: default

URxvt_scratchpad.font: -*-terminus-medium-r-*-*-18-*-*-*-*-*-iso10646-1
URxvt_scratchpad.boldFont: -*-terminus-bold-r-*-*-18-*-*-*-*-*-iso10646-1

URxvt_scratchpad.scrollstyle: plain
URxvt_scratchpad.scrollBar: false
URxvt_scratchpad.cursorBlink: false
URxvt_scratchpad.cursorUnderline: false

URxvt_scratchpad.color0:  #1c1c1c
URxvt_scratchpad.color8:  #2B2B2B
URxvt_scratchpad.color1:  #A5372E
URxvt_scratchpad.color9:  #E06C5C
